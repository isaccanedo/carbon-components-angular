<section class="tsd-panel tsd-member {{cssClasses}}">
	<a name="{{anchor}}" class="tsd-anchor"></a>
	{{#if name}}
		<h3>{{#each flags}}<span class="tsd-flag ts-flag{{this}}">{{this}}</span> {{/each}}{{{wbr name}}}</h3>
	{{/if}}

	{{#if decorators}}
		<div class="tsd-signature" style="border-bottom: none; margin-bottom: 0; padding-left: 20px;">
			{{#each decorators}}
				<span>
					{{#isEmpty arguments}}
						@{{name}}()
					{{else}}
						@{{name}}({{toJSON arguments}})
					{{/isEmpty}}
				</span>
				<br />
			{{/each}}
		</div>
	{{/if}}

	{{#if signatures}}
		{{> member.signatures}}
	{{else}}{{#if hasGetterOrSetter}}
		{{> member.getterSetter}}
	{{else}}{{#if isReference}}
		{{> member.reference}}
	{{else}}
		{{> member.declaration}}
	{{/if}}{{/if}}{{/if}}

	{{#each groups}}
		{{#each children}}
			{{#unless hasOwnDocument}}
				{{> member}}
			{{/unless}}
		{{/each}}
	{{/each}}
</section>
